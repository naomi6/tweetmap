<h3>Tweets using the word "<%= request.params[:word] %>" near you:</h3>

<ul class="media-list">
  <% @tweets.each do |tweet| %>
    <li class="media">
      <%= image_tag(tweet["profile_image_url"]) %>
      
      <strong>@<%= tweet["from_user"] %></strong>
      <%= tweet["text"] %>
    </li>
  <% end %>
</ul>